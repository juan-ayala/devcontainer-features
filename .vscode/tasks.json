{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Devcontainer: test-autogenerated",
            "type": "shell",
            "command": "devcontainer",
            "args": [
                "features",
                "test",
                "--features",
                "aem-cloud",
                "--skip-duplicated",
                "--base-image",
                "mcr.microsoft.com/devcontainers/base",
                "--skip-scenarios"
            ],
            "group": "test"
        },
        {
            "label": "Devcontainer: test-scenarios",
            "type": "shell",
            "command": "devcontainer",
            "args": [
                "features",
                "test",
                "--features",
                "aem-cloud",
                "--skip-duplicated",
                "--skip-autogenerated"
            ],
            "group": "test"
        }
    ]
}